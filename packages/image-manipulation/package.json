{
  "name": "@file-storage/image-manipulation",
  "version": "1.3.4-alpha.0",
  "description": "Image manipulation for file-storage",
  "author": "Dang Nguyen <haidang009@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/googlicius/file-storage.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile:cjs && yarn compile:esm",
    "clean": "rimraf -rf ./dist",
    "compile:cjs": "tsc -p tsconfig-cjs.build.json",
    "compile:esm": "tsc -p tsconfig-esm.build.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:detectOpenHandles": "jest --detectOpenHandles"
  },
  "dependencies": {
    "sharp": "^0.28.3"
  },
  "devDependencies": {
    "@file-storage/common": "^1.3.4-alpha.0",
    "@types/sharp": "^0.28.5",
    "rimraf": "~3.0.2",
    "typescript": "~4.3.5"
  }
}
